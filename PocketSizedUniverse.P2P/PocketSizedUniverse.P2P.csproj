<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <AssemblyTitle>PocketSizedUniverse P2P Protocol</AssemblyTitle>
    <AssemblyDescription>Peer-to-peer messaging protocol for PocketSizedUniverse</AssemblyDescription>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MessagePack" Version="2.6.100" />
    <PackageReference Include="MessagePack.Annotations" Version="2.6.100" />
    <PackageReference Include="System.Security.Cryptography.Algorithms" Version="4.3.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="../PocketSizedUniverseAPI/PocketSizedUniverse.API.csproj" />
  </ItemGroup>

</Project>
